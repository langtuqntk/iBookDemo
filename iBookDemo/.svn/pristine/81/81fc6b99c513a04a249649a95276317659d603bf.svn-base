/*Adding CSS display flags*/
.flag-display{
	z-index:99;
	position:absolute;
	padding-left:10px
}
.flag-image{
		/*opacity:0.3;*/
		width:30px;
		height:30px;
		border:none;
		background:none;
		
	}
	.flag-image:hover{
		opacity:1;
	}
	.flag-image:active{		
		transform: translateY(2px);
	}
	.flag-image:focus{
		outline:none;
	}
/*End CSS display flags*/	